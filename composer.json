{
    "name": "hollyit/filament-static-assets",
    "description": "Improved handling of stylesheets and javascript files for Filament",
    "keywords": [
        "hollyit",
        "filament",
        "assets",
        "performance",
        "laravel"
    ],
    "homepage": "https://github.com/HollyIT/filament-static-assets",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Holly",
            "email": "jamie@hollyit.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.13",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "orchestra/testbench": "7.*",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "HollyIT\\FilamentStaticAssets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HollyIT\\FilamentStaticAssets\\Tests\\": "tests/"
        }

    },

    "extra": {
        "laravel": {
            "providers": [
                "HollyIT\\FilamentStaticAssets\\StaticAssetsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
